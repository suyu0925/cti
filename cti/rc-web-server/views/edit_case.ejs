                                            <option value="3.客户拒绝还款">3.客户拒绝还款</option>
                                            <option value="4.申请上门外访">4.申请上门外访</option>
                                            <option value="5.号码错误">5.号码错误</option>
                                        </select>
                                    </div>
                                <textarea form="log_form" id="log-textarea" name="log_textarea" cols="" rows=""
                                          style="width:99%;height:130px; margin-top:2px;resize:none"></textarea>
                                    <br/>

                                    <div align="right">
                                        <input type="submit" id="log-button" value="提交"/>
                                    </div>
                                </fieldset>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
